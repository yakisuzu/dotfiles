{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [],
  "parserOptions": {},
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "WScript": true
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": ["error", {"args": "none"}],
    "semi": "error",
    "valid-jsdoc": ["warn", {"requireParamDescription": false, "requireReturnDescription": false}],
    "eqeqeq": "error",
    "comma-dangle": ["warn", "always-multiline"]
  }
}
